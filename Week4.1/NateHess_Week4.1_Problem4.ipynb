    "def calculateHandlen(hand):\n",
    "    \"\"\" \n",
    "    Returns the length (number of letters) in the current hand.\n",
    "    \n",
    "    hand: dictionary (string int)\n",
    "    returns: integer\n",
    "    \"\"\"\n",
    "    handCount = 0\n",
    "    \n",
    "    for i in hand.values():\n",
    "        handCount += i\n",
    "\n",
    "    return handCount\n"
   ]
